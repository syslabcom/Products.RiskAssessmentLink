<?xml version="1.0"?>

<object name="portal_skins"
        meta_type="Plone Skins Tool"
        allow_any="False"
        cookie_persistence="False"
        request_varname="plone_skin">

 <object name="riskassessmentlink_templates"
         meta_type="Filesystem Directory View"
         directory="RiskAssessmentLink/skins/riskassessmentlink_templates"/>
 <object name="riskassessmentlink_images"
         meta_type="Filesystem Directory View"
         directory="RiskAssessmentLink/skins/riskassessmentlink_images"/>
 <object name="riskassessmentlink_styles"
         meta_type="Filesystem Directory View"
         directory="RiskAssessmentLink/skins/riskassessmentlink_styles"/>
 
 <skin-path name="Plone Default">
  <layer insert-after="custom" 
         name="riskassessmentlink_templates"/>
  <layer insert-after="custom" 
         name="riskassessmentlink_images"/>
  <layer insert-after="custom" 
         name="riskassessmentlink_styles"/>
 </skin-path>

</object>